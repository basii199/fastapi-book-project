{
  "services": [
    {
      "name": "nginx",
      "buildCommand": "docker build -f Dockerfile.nginx -t my-nginx .",
      "startCommand": "docker run -p 80:80 my-nginx"
    },
    {
      "name": "fastapi",
      "buildCommand": "pip install -r requirements.txt",
      "startCommand": "uvicorn main:app --host 0.0.0.0 --port 8000"
    }
  ]
}
